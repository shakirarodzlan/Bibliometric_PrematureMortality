@article{ WOS:000532015400001,
Author = {Gilderbloom, John Hans and Meares, Wesley L. and Squires, Gregory D.},
Title = {Pollution, place, and premature death: evidence from a mid-sized city},
Journal = {LOCAL ENVIRONMENT},
