@article{ WOS:000366724000017,
Author = {Wang, Dong D. and Li, Yanping and Chiuve, Stephanie E. and Hu, Frank B.
   and Willett, Walter C.},
Title = {Improvements In US Diet Helped Reduce Disease Burden And Lower Premature
   Deaths, 1999-2012; Overall Diet Remains Poor},
Journal = {HEALTH AFFAIRS},
Year = {2015},
Volume = {34},
