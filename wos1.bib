@article{ WOS:000496924300006,
Author = {Butsing, Nipaporn and Tipayamongkholgul, Mathuros and Ratanakorn, Disya},
Title = {Estimation of expected years of life lost for patients with ischemic
   stroke and intracerebral hemorrhage},
