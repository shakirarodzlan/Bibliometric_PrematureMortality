@article{ WOS:000371388600006,
Author = {Hirvikoski, Tatja and Mittendorfer-Rutz, Ellenor and Boman, Marcus and
   Larsson, Henrik and Lichtenstein, Paul and Bolte, Sven},
Title = {Premature mortality in autism spectrum disorder},
Journal = {BRITISH JOURNAL OF PSYCHIATRY},
