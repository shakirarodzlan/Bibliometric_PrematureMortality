@article{ WOS:A1993LY83500011,
Author = {LAWSON, JS and BLACK, D},
Title = {SOCIOECONOMIC-STATUS - THE PRIME INDICATOR OF PREMATURE DEATH IN
   AUSTRALIA},
Journal = {JOURNAL OF BIOSOCIAL SCIENCE},
