@article{ WOS:000740227600006,
Author = {Liu, Jianwei and Cao, Hongbin and Zhang, Yali and Chen, Hui},
Title = {Potential years of life lost due to PM2.5-bound toxic metal exposure:
   Spatial patterns across 60 cities in China},
Journal = {SCIENCE OF THE TOTAL ENVIRONMENT},
Year = {2022},
Volume = {812},
Month = {MAR 15},
Abstract = {To clarify the spatial patterns of disease burden caused by toxic metals
   in line particulate matter (PM2.5) across China, annual concentration
   levels of typical toxic metals in PM2.5 over 60 cities of China were
   retrieved. Then, potential years of life lost (PYLL) attributable to
   toxic metal (As, Cd, Cr (VI), Mn, and Ni) exposure was calculated from
   health risk assessments and lifetable estimates. The results show that
   Cr(VI) and As were the most polluted metals and greatly exceeded the
   recommended annual values in the National Ambient Air Quality Standard
