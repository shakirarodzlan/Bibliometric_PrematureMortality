   King, Glenn F. and Petrou, Steven},
Title = {Selective Na(V)1.1 activation rescues Dravet syndrome mice from seizures
   and premature death},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2018},
