@article{ WOS:000272385800002,
Author = {Lee, Robert},
Title = {Early Death and Long Life in History: Establishing the Scale of
   Premature Death in Europe and its Cultural, Economic and Social
   Significance},
Journal = {HISTORICAL SOCIAL RESEARCH-HISTORISCHE SOZIALFORSCHUNG},
Year = {2009},
Volume = {34},
Number = {4, SI},
Pages = {23-60},
Abstract = {``Fruher Tod und langes Leben in historischer Perspektive. Der
   vorzeitige Tod in Europa und seine kulturelle, okonomische und soziale
   Bedeutung{''}. Up until the late-eighteenth century and beyond,
   premature death was a constant reality for individuals and their
   families irrespective of their social standing rather than an
   exceptional occurrence. Premature death is a theme which has given rise
   to wide-ranging discussions relating to its economic and social
   significance, its interpretation and commemoration, its cultural and
   political representation, and the role of medical practitioners and
   other professional groups in limiting its familial and societal impact.
   This paper will analyse the process by which the scale of premature
   death was scientifically established from the early eighteenth century
   onwards; examine the key factors which influenced the growing concern
   over the extent of truncated longevity; explore the ways in which
   private perceptions of premature death evolved over time; assess the
   role of human agency in responding to an avoidable loss of life; and
   discuss the cultural, economic and social significance of premature
   death.},
