@article{ WOS:000538789800011,
Author = {Klofas, Lindsay K. and Short, Brittany P. and Zhou, Chengwen and Carson,
   Robert P.},
Title = {Prevention of premature death and seizures in a Depdc5 mouse epilepsy
   model through inhibition of mTORC1},
Journal = {HUMAN MOLECULAR GENETICS},
