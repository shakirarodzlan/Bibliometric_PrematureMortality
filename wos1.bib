Type = {Article},
Language = {English},
Affiliation = {Amaddeo, P (Corresponding Author), Ctr Med Sport, Bgo Palazzo 130, I-24125 Bergamo, Italy.
   Prometti, P., FSM Ist Lumezzane, Brescia, Italy.
   Prometti, P.; Bellini, G., Univ Brescia, Scuola Specializzaz Med Sport, Brescia, Italy.
   Amaddeo, P., Azienda Osped Papa Giovanni XXIII, Ctr Med Sport, Bergamo, Italy.},
ISSN = {0025-7826},
