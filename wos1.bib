@article{ WOS:000239051400002,
Author = {van Dam, Rob M. and Willett, Walter C. and Manson, JoAnn E. and Hu,
   Frank B.},
Title = {The relationship between overweight in adolescence and premature death
   in women},
Journal = {ANNALS OF INTERNAL MEDICINE},
