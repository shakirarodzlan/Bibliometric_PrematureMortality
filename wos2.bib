@article{ WOS:000383100300001,
Author = {Silva, Raquel A. and West, J. Jason and Lamarque, Jean-Francois and
   Shindell, Drew T. and Collins, William J. and Dalsoren, Stig and
   Faluvegi, Greg and Folberth, Gerd and Horowitz, Larry W. and Nagashima,
   Tatsuya and Naik, Vaishali and Rumbold, Steven T. and Sudo, Kengo and
