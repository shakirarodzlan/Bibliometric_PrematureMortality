@article{ WOS:000448805600032,
Author = {Watila, Musa M. and Balarabe, Salisu A. and Ojo, Olubamiwo and Keezer,
   Mark R. and Sander, Josemir W.},
Title = {Overall and cause-specific premature mortality in epilepsy: A systematic
   review},
