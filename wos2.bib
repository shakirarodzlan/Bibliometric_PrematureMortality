@article{ WOS:000283507700004,
Author = {Neovius, Kristian and Rasmussen, Finn and Sundstrom, Johan and Neovius,
   Martin},
Title = {Forecast of future premature mortality as a result of trends in obesity
   and smoking: nationwide cohort simulation study},
Journal = {EUROPEAN JOURNAL OF EPIDEMIOLOGY},
