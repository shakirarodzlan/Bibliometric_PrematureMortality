@article{ WOS:000310592900004,
Author = {Beatriz Jimenez, Raquel and Caceres Bronfman, Nicolas},
Title = {Comprehensive indicators of traffic-related premature mortality},
Journal = {JOURNAL OF RISK RESEARCH},
