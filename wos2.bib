@article{ WOS:000266890800010,
Author = {Kriegbaum, Margit and Christensen, Ulla and Lund, Rikke and Osler,
   Merete},
Title = {Job Losses and Accumulated Number of Broken Partnerships Increase Risk
   of Premature Mortality in Danish Men Born in 1953},
Journal = {JOURNAL OF OCCUPATIONAL AND ENVIRONMENTAL MEDICINE},
Year = {2009},
Volume = {51},
Number = {6},
Pages = {708-713},
Month = {JUN},
Abstract = {Objective: To investigate how accumulation of job losses and broken
   partnerships affect the risk of premature mortality, and to study joint
   exposure to both events. Methods: Birth cohort study of 9789 Danish men
   born in 1953 with follow-up of events between the ages of 40 and 5.1.
   Results: The adjusted hazard rates for premature mortality was 1.44
   (95\% CI = 1.15 to 1.80) for individuals with one job loss, 1.55 (1.13
   to 2.13) for individuals with one broken partnership, and 2.15 (95 \% CI
   = 1.49 to 3.10) for individuals with two or more broken partnerships.
   Conclusions: Experience of at least one job loss increased the risk of
   premature mortality. The risk of Premature mortality increased with the
   number of broken partnerships. There was no statistical interaction
   between job losses and broken partnerships. (J Occup Environ Med.
   2009;51:708-713)},
