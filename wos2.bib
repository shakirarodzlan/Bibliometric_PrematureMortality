@article{ WOS:A1997XY23500023,
Author = {Barnett, E and Armstrong, DL and Casper, ML},
Title = {Social class and premature mortality among men: A method for state-based
   surveillance},
Journal = {AMERICAN JOURNAL OF PUBLIC HEALTH},
Year = {1997},
Volume = {87},
Number = {9},
Pages = {1521-1525},
Month = {SEP},
Abstract = {Objectives. This study examined trends in mortality by social class for
   Black and White men aged 35 through 54 years in North Carolina, for 1984
   through 1993, using an inexpensive, newly developed state-based
   surveillance method.
   Methods. Data from death certificates and census fries permitted
   examination of four social classes, defined on the basis of occupation.
   Results. Premature mortality was inversely associated with social class
   for both Blacks and Whites. Blacks were at least twice as likely to die
   as Whites within each social class.
   Conclusions. Adoption of state specific surveillance of social class and
   premature mortality would provide data crucial for developing and
   evaluating Public health programs to reduce social inequalities in
   health.},
