@article{ WOS:000345227400020,
Author = {Kaiboriboon, Kitti and Schiltz, Nicholas K. and Bakaki, Paul M. and
   Lhatoo, Samden D. and Koroukian, Siran M.},
Title = {Premature mortality in poor health and low income adults with epilepsy},
Journal = {EPILEPSIA},
