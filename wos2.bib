@article{ WOS:000175775400011,
Author = {Cooper, RS and Kennelly, JF and Durazo-Arvizu, R and Oh, HJ and Kaplan,
   G and Lynch, J},
Title = {Relationship between premature mortality and socioeconomic factors in
   black and white populations of US metropolitan areas},
Journal = {PUBLIC HEALTH REPORTS},
